{
  "name": "es6-module",
  "version": "1.0.0",
  "description": "An ES6 module boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint ./src/**/*.js",
    "docs": "esdoc",
    "dev": "npm run amd:watch & npm run common:watch",
    "dist": "npm run lint && npm run test && npm run amd && npm run common",
    "amd": "BABEL_ENV=amd babel src --out-dir dist/amd",
    "amd:watch": "BABEL_ENV=amd babel src --out-dir dist/amd --watch",
    "common": "BABEL_ENV=common babel src --out-dir dist/common",
    "common:watch": "BABEL_ENV=common babel src --out-dir dist/common --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsakas/es6-module.git"
  },
  "keywords": [
    "es6",
    "module",
    "boilerplate"
  ],
  "author": "Jon Sakas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsakas/es6-module/issues"
  },
  "homepage": "https://github.com/jsakas/es6-module#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2",
    "requirejs": "^2.3.3"
  }
}
